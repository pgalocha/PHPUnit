{
  "name": "home/php-unit",
  "description": "PHPUnit Course System",
  "type": "project",
  "require": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^9",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "CodeCourse\\": "src/CodeCourse",
      "Helpers\\": "src/core",
      "Model\\": "src/models"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
